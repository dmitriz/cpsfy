{
  "version": "2.1.2",
  "name": "cpsfy",
  "description": "Tiny goodies for Continuation-Passing-Style functions",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": "https://github.com/dmitriz/cpsfy.git",
  "author": "Dmitri Zaitsev <dmitri14@gmail.com> (https://github.com/dmitriz)",
  "license": "MIT",
  "scripts": {
    "start": "git ac; npm version",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git pull --rebase && git push origin --all; git push origin --tags",
    "test": "npm run tape",
    "ava": "TEST=ava nyc ava test/*test.js",
    "ava-w": "nyc ava test/*test.js -w",
    "tape": "nyc tape test/*test.js",
    "tape-w": "nyc tape-watch test/*test.js",
    "tap": "tap test/*test.js",
    "sync": "git ac && git pull && git push",
    "toc": "markdown-toc README.md > toc"
  },
  "keywords": [
    "continuation",
    "functional",
    "monad",
    "applicative",
    "functor",
    "stream",
    "promise",
    "async"
  ],
  "devDependencies": {
    "ava": "2.0.0-rc.1",
    "markdown-toc": "1.2.0",
    "nyc": "14.1.1",
    "tape": "4.10.2"
  },
  "dependencies": {}
}
