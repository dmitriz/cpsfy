  "description": "Tiny goodies for Continuation-Passing-Style functions",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitriz/tiny-cps"
  },
  "scripts": {
    "start": "npm version",
    "preversion": "npm test",
    "postversion": "npm publish",
