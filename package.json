{
  "version": "3.0.0",
  "name": "cpsfy",
  "description": "Tiny goodies for Continuation-Passing-Style functions",
  "main": "index.js",
  "files": [
    "index.js",
    "utils.js"
  ],
  "repository": "https://github.com/dmitriz/cpsfy.git",
  "author": "Dmitri Zaitsev <dmitri14@gmail.com> (https://github.com/dmitriz)",
  "license": "MIT",
  "scripts": {
    "start": "git add . && git commit; npm version",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git pull --rebase && git push origin --all; git push origin --tags",
    "test": "npm run tape",
    "ava": "TEST=ava ava test/*test.js",
    "avaw": "TEST=ava nyc ava test/*test.js -w",
    "tape": "nyc tape test/*test.js",
    "tapew": "nyc tape-watch test/*test.js",
    "tap": "tap test/*test.js",
    "sync": "git add . && git commit; git pull && git push",
    "com": "git add . && git cz",
    "tocmanual": "npx markdown-toc README.md > toc",
    "toc": "md-magic --path 'DOCUMENTATION.md'",
    "doc": "documentation build 'index.js' -f html -o auto-docs"
  },
  "keywords": [
    "library",
    "utilities",
    "function",
